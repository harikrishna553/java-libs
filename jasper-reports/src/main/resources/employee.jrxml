<jasperReport name="hello" language="java" pageWidth="595" pageHeight="842"
    columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">

    <field name="EMPLOYEE_ID" class="java.lang.Integer"/>
    <field name="EMPLOYEE_BDATE" class="java.util.Date"/>
    <field name="EMPLOYEE_DESIGNATION" class="java.lang.String"/>
    <field name="EMPLOYEE_EMPNAME" class="java.lang.String"/>
